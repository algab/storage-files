{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": 0,
        "linebreak-style": 0,
        "indent": 0,
        "class-methods-use-this": 0,
        "no-param-reassign": 0,
        "object-curly-newline": 0
    }
}